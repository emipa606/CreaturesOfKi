<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <ThingDef ParentName="EggFertBase">
    <defName>EggTeshiFertilized</defName>
    <label>teshi egg (fert.)</label>
    <description>A fertilized teshi egg. If all goes well, it should hatch into a baby teshi. It can be eaten raw, but it's much better cooked.</description>
    <graphicData>
      <color>(230,230,230)</color>
      <drawSize>2.0</drawSize>
    </graphicData>
    <statBases>
      <MarketValue>125.0</MarketValue>
    </statBases>
    <comps>
      <li Class="CompProperties_Hatcher">
        <hatcherDaystoHatch>15</hatcherDaystoHatch>
        <hatcherPawn>Teshi</hatcherPawn>
      </li>
    </comps>
  </ThingDef>
</Defs>